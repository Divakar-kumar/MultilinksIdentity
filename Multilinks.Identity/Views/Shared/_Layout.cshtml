<!DOCTYPE html>
<html>
<head lang="en">
   <meta charset="utf-8" />
   <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

   <title>Multilinks | Create Connect Communicate</title>

   <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
   <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
   <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">
   <link rel="icon" href="favicon.ico" type="image/x-icon">

   <link rel="stylesheet" href="../css/bootstrap.css">
   <link rel="stylesheet" href="../css/styles.css">

   <script src="../js/jquery.slim.min.js"></script>
   <script src="../js/bootstrap.min.js"></script>
</head>
<body>
   <div class="page">
      <!--========================================================
                                HEADER
      =========================================================-->
      <header>
         <nav class="navbar navbar-expand-lg fixed-top text-uppercase" id="mainNav">
            <div class="container">
               <div class="brand">
                  <a class="navbar-brand" asp-controller="Home" asp-action="PortalHome">Multilinks</a>
               </div>
            </div>
         </nav>
      </header>

      <!--========================================================
                                CONTENT
      =========================================================-->
      <main>
         @RenderBody()
      </main>

      <!--========================================================
                                FOOTER
      =========================================================-->
      <footer class="footer text-center">
         <div class="container py-4">
            <div class="copyright">
               Copyright MDS Multilinks © 2019
            </div>
         </div>
      </footer>
   </div>

   @RenderSection("Scripts", required: false)
</body>
</html>
